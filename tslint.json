{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ],
    "arrow-parens": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "trailing-comma": true,
    "max-line-length": false,
    "object-literal-key-quotes": false
  },
  "rulesDirectory": []
}
